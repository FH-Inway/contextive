{
    "cSpell.words": [
        "amodio",
        "Contextive",
        "danielsanmedium",
        "dbaeumer",
        "devcycles",
        "dorny",
        "dotnettools",
        "dscodegpt",
        "Expecto",
        "fgrep",
        "formulahendry",
        "hbenl",
        "Intelli",
        "ionide",
        "MADR",
        "paket",
        "parens",
        "singularised",
        "sinonjs",
        "vimscript",
        "xvfb",
        "yzhang"
    ],
    "FSharp.dotnetRoot": "/usr/share/dotnet",
    "FSharp.workspacePath": "./src/Contextive.sln",
    "vsCodeExtensionPath": "./src/vscode/contextive",
    "languageServerPath": "./src/Contextive.LanguageServer",
    "languageServerTestsPath": "./src/Contextive.LanguageServer.Tests",
    "files.exclude": {
        "out": false, // set this to true to hide the "out" folder with the compiled JS files
        "dist": false // set this to true to hide the "dist" folder with the compiled JS files
    },
    "search.exclude": {
        "out": true, // set this to false to include "out" folder in search results
        "dist": true // set this to false to include "dist" folder in search results
    },
    "dotnet-test-explorer.autoWatch": true,
    "dotnet-test-explorer.testProjectPath": "**/Contextive.LanguageServer.Tests.fsproj",
    "FSharp.enableAdaptiveLspServer": true,
    "mochaExplorer.logpanel": true,
    "mochaExplorer.files": "**/out/*.main.test.js",
    "mochaExplorer.launcherScript": "src/vscode/contextive/out/runTest",
    "mochaExplorer.autoload": true,
    "mochaExplorer.ipcTimeout": 20000,
    "mochaExplorer.ipcRole": "server",
    "mochaExplorer.env": {
        "VSCODE_VERSION": "insiders",
        "ELECTRON_RUN_AS_NODE": null,
        "DISPLAY": ":99.0",
        "XDG_RUNTIME_DIR":"/tmp",
        //"MOCHA_FGREP": "Multi-Root" // Will default to single-root.  Uncomment when testing multi-root from the test explorer.
    },
    "mochaExplorer.exit": true,
    "testExplorer.useNativeTesting": false,
    "testing.openTesting": "neverOpen"
}